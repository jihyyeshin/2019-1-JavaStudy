package Q19;

public class Account {
	Account(){
		int MIN_BALANCE=0;
		int MAX_BALANCE=1000000;
	}
	
	private int balance;
	
	public int getBalance() {
		if (balance>=0 && balance<=1000000) 
			return balance;
		else
			return 0;
	}
	public void setBalance(int balance) {
		this.balance = balance;
	}
	
	
}
